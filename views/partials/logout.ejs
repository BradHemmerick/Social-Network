<button id="log_out_user" type="button">Log Out</button>
<button id="return_to_home" type="button">Return to Home</button>
<script>
	$("#log_out_user").on("click",function(){
		$.ajax({
			url: "/logout",
			method: "POST", 
			success: function(){
				location.href = "/";
			}
		})
	});

	$("#return_to_home").on("click",function(){
		location.href = "/home";
	});
</script>